{
	"extends": ["//"],
	"tasks": {
		"build": {
			"env": ["DATABASE_URL"],
			"outputs": ["dist/**"]
		},
		"db:migrate:dev": {
			"env": ["DATABASE_URL"],
			"cache": false,
			"persistent": true
		},
		"db:migrate:deploy": {
			"env": ["DATABASE_URL"],
			"cache": false
		},
		"db:push": {
			"env": ["DATABASE_URL"],
			"cache": false
		},
		"db:seed": {
			"env": ["DATABASE_URL"],
			"cache": false
		},
		"db:generate": {
			"env": ["DATABASE_URL"],
			"cache": false
		}
	}
}
