{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"db:migrate:dev": {
			"cache": false,
			"persistent": true
		},
		"db:migrate:deploy": {
			"cache": false
		},
		"db:push": {
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"db:generate": {
			"cache": false
		},
		"dev": {
			"dependsOn": ["^db:generate"],
			"cache": false,
			"persistent": true
		},
		"lint": {}
	}
}
